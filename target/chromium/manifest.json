{
  "manifest_version": 3,
  "name": "extension-builder",
  "version": "0.0.1",
  "version_name": "Beta",
  "description": "A awesome tab overrider extension.",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/icon.png",
      "24": "icons/icon.png",
      "32": "icons/icon.png"
    }
  },
  "icons": {
    "128": "icons/icon.png",
    "16": "icons/icon.png",
    "256": "icons/icon.png",
    "48": "icons/icon.png"
  },
  "permissions": [
    "storage"
  ]
}
